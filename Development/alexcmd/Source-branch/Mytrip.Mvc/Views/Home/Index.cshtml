@{
    View.Title = Html.MytripPageTitle(Model.title, "/");
    View.Keywords=Html.MytripPageTitle(Model.title, ",");
    View.Description=Html.MytripPageTitle(Model.title, ",");
}
@section csslink{}
@section scriptlink{
@Html.ScriptLink("MicrosoftAjax.js,MicrosoftMvcAjax.js")
}
@section contentBig{
@if (!Model.developer){
@Html.Partial("_homePage")
}
}
@section contentSmall{
@if (!Model.developer){
@Html.Partial("_sideBar")
} 
}
@section content{
@if (Model.developer){
@Html.Partial("_license")
} 
}